{
	"name": "@snickbit/polymine",
	"version": "0.0.4",
	"private": true,
	"license": "MIT",
	"main": "bin/polymine",
	"bin": {
		"polymine": "bin/polymine"
	},
	"files": [
		"bin",
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"clean": "shx rm -rf bin",
		"depcheck": "depcheck",
		"prod": "pnpm run build --minify",
		"restart": "nodemon bin/polymine --out:polymine=5",
		"start": "node bin/polymine",
		"watch": "pnpm run build --watch"
	},
	"dependencies": {
		"@snickbit/node-cli": "3.0.24",
		"@snickbit/node-utilities": "4.4.10",
		"@snickbit/out": "2.0.48",
		"dockerode": "3.3.4",
		"raknet": "1.8.0"
	},
	"devDependencies": {
		"@types/node": "18.7.1",
		"depcheck": "1.4.3",
		"nodemon": "2.0.20",
		"shx": "0.3.4",
		"tsup": "6.2.3",
		"typescript": "4.8.2"
	},
	"engines": {
		"node": ">= 12"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"clean": true,
		"dts": false,
		"format": [
			"cjs"
		]
	}
}
